<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="LamdaTest automtion regression suite" thread-count="2"
	parallel="tests" verbose="3">

	<test name="Test On Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="110.0" />
		<parameter name="platform" value="Windows 11" />

		<classes>
			<class name="LamdaTest.SimpleFormDemoTest" />
			<class name="LamdaTest.DragDropSlidersTest" />
		</classes>


	</test> <!--
	Test -->

	<test name="Test On Firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="version" value="110.0" />
		<parameter name="platform" value="Windows 11" />

		<classes>
			<class name="LamdaTest.SimpleFormDemoTest" />
			<class name="LamdaTest.DragDropSlidersTest" />
		</classes>
	</test>
</suite> <!--
Suite -->
